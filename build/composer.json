{
    "name": "dfwp/babar",
    "authors": [
        {
            "name": "Cl√©ment Biron",
            "email": "clement.biron@gmail.com"
        }
    ],
    "config": {
        "vendor-dir": "../vendor",
        "disable-tls": true,
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "Doublefou\\": "../vendor/dfwp/doublefou"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/BeAPI/bea-sanitize-filename"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "dfwp/doublefou" : "dev-master",
        "filp/whoops": "^2.1",
        "php-ds/php-ds": "^1.2",
        "bea/sanitize-filename": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "../../../plugins/{$name}": ["bea/sanitize-filename"]
        }
    }
}
